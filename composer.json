{
    "name": "asamoahboateng/frankenphp-deploy",
    "description": "FrankenPHP + Traefik Docker deployment scaffolding for Laravel Octane projects.",
    "keywords": ["laravel", "frankenphp", "docker", "octane", "traefik", "deployment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Asamoah Boateng",
            "email": "asamoahboateng@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AsamoahBoateng\\FrankenPhpDeploy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AsamoahBoateng\\FrankenPhpDeploy\\FrankenPhpDeployServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
